<Window x:Class="GameListWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameListWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="MainWindow_Label_TitreBienvenue" Content="Bienvenue sur GameList" HorizontalAlignment="Center" Margin="0,32,0,0" VerticalAlignment="Top" FontSize="18"/>
        <Label x:Name="MainWindow_Label_Login" Content="Login" HorizontalAlignment="Left" Margin="256,128,0,0" VerticalAlignment="Top"/>
        <Label x:Name="MainWindow_Label_Mdp" Content="Mot de passe" HorizontalAlignment="Left" Margin="256,160,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="MainWindows_TextBox_InputLogin" HorizontalAlignment="Left" Height="23" Margin="376,132,0,0" TextWrapping="Wrap" Text="Pseudo" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="MainWindow_TextBox_InputMdp" HorizontalAlignment="Left" Height="23" Margin="376,164,0,0" TextWrapping="Wrap" Text="Mot de passe" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
